<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>PB_TentWall</defName>
		<label>tent wall</label>
		<description>An impassable tent wall. Tent poles need to be built as this is not enough to support the tent.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<minifiedDef>PB_MinifiedThing_10Stackable</minifiedDef>
		<thingCategories>
			<li>Buildings</li>
		</thingCategories>
		<graphicData>
			<texPath>PortableBlueprint/TentWall/TentWall_Atlas</texPath>
			<graphicClass>PortableBlueprint.Tent.Graphic_Tent</graphicClass>
			<linkFlags>
				<li>Custom1</li>
			</linkFlags>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<uiIconPath>PortableBlueprint/TentWall/TentWallFabric_MenuIcon</uiIconPath>
		<uiIconPathsStuff>
			<li>
				<appearance>Fabric</appearance>
				<iconPath>PortableBlueprint/TentWall/TentWallFabric_MenuIcon</iconPath>
			</li>
			<li>
				<appearance>Leathery</appearance>
				<iconPath>PortableBlueprint/TentWall/TentWallLeathery_MenuIcon</iconPath>
			</li>
		</uiIconPathsStuff>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>50</WorkToBuild>
			<Mass>0.6</Mass>
			<Flammability>1.0</Flammability>
			<MeditationFocusStrength>0.22</MeditationFocusStrength>
		</statBases>
		<costStuffCount>10</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Building</altitudeLayer>
		<passability>Impassable</passability>
		<blockWind>true</blockWind>
		<castEdgeShadows>true</castEdgeShadows>
		<fillPercent>1</fillPercent>
		<coversFloor>false</coversFloor>
		<placingDraggableDimensions>1</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<holdsRoof>false</holdsRoof>
		<designationCategory>Furniture</designationCategory>
		<uiOrder>8000</uiOrder>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>true</blockLight>
		<canOverlapZones>false</canOverlapZones>
		<fertility>0</fertility>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<building>
			<paintable>true</paintable>
			<isInert>true</isInert>
			<isEdifice>true</isEdifice>
			<isPlaceOverableWall>true</isPlaceOverableWall>
			<ai_chillDestination>false</ai_chillDestination>
			<supportsWallAttachments>false</supportsWallAttachments>
			<blueprintGraphicData>
				<texPath>PortableBlueprint/TentWall/TentWall_Blueprint</texPath>
				<graphicClass>Graphic_Single</graphicClass>
			</blueprintGraphicData>
			<relatedBuildCommands>
				<li>Door</li>
			</relatedBuildCommands>
		</building>
		<comps>
			<li Class="CompProperties_MeditationFocus">
				<statDef>MeditationFocusStrength</statDef>
				<focusTypes>
					<li>Minimal</li>
				</focusTypes>
			</li>
		</comps>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>3</multiplier>
			</li>
			<li>
				<damageDef>Thump</damageDef>
				<multiplier>3</multiplier>
			</li>
		</damageMultipliers>
		<modExtensions>
			<li Class="PortableBlueprint.Tent.TentParts"></li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>PB_TentPole</defName>
		<label>tent pole</label>
		<description>A pole that can be used to pitch a tent within its range. It cannot support any other roof.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<minifiedDef>PB_MinifiedThing_10Stackable</minifiedDef>
		<thingCategories>
			<li>Buildings</li>
		</thingCategories>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Furniture</designationCategory>
		<uiOrder>8001</uiOrder>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.05</fillPercent>
		<pathCost>25</pathCost>
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<WorkToBuild>200</WorkToBuild>
			<Mass>2.5</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>5</Beauty>
		</statBases>
		<costList>
			<Steel>10</Steel>
		</costList>
		<holdsRoof>false</holdsRoof>
		<canOverlapZones>false</canOverlapZones>
		<rotatable>false</rotatable>
		<fertility>0</fertility>
		<specialDisplayRadius>6.9</specialDisplayRadius>
		<building>
			<isInert>true</isInert>
			<isEdifice>true</isEdifice>
			<isPlaceOverableWall>true</isPlaceOverableWall>
			<ai_chillDestination>false</ai_chillDestination>
			<paintable>true</paintable>
		</building>
		<graphicData>
			<drawSize>(1.6,1.3)</drawSize>
			<drawOffset>(0,0,0.6)</drawOffset>
			<texPath>PortableBlueprint/TentPole</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shadowData>
				<volume>(0.1, 2, 0.1)</volume>
				<offset>(0,0,0.1)</offset>
			</shadowData>
			<damageData>
				<rect>(0.2,0.2,0.6,0.6)</rect>
			</damageData>
		</graphicData>
		<comps>
			<li>
				<compClass>PortableBlueprint.Tent.TentPoleComp</compClass>
			</li>
		</comps>
		<modExtensions>
			<li Class="PortableBlueprint.Tent.TentParts"></li>
		</modExtensions>
	</ThingDef>

	<ThingDef Name="TentDoor" ParentName="DoorBase">
		<defName>PB_TentDoor</defName>
		<label>tent door</label>
		<description>A door to divide the inside of a tent from the outside.</description>
		<thingClass>PortableBlueprint.Tent.Building_TentDoor</thingClass>
		<category>Building</category>
		<minifiedDef>PB_MinifiedThing_10Stackable</minifiedDef>
		<thingCategories>
			<li>Buildings</li>
		</thingCategories>
		<holdsRoof>false</holdsRoof>
		<fillPercent>1</fillPercent>
		<statBases>
			<MaxHitPoints>75</MaxHitPoints>
			<WorkToBuild>100</WorkToBuild>
			<Mass>0.6</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<graphicData>
			<texPath>PortableBlueprint/TentDoor/TentDoor_MenuIcon/TentDoor_Fabric</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<linkFlags>
				<li>Custom1</li>
			</linkFlags>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<stuffCategories Inherit="False">
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<uiOrder>8002</uiOrder>
		<uiIconPath>PortableBlueprint/TentDoor/TentDoor_MenuIcon/TentDoor_Fabric</uiIconPath>
		<uiIconPathsStuff>
			<li>
				<appearance>Fabric</appearance>
				<iconPath>PortableBlueprint/TentDoor/TentDoor_MenuIcon/TentDoor_Fabric</iconPath>
			</li>
			<li>
				<appearance>Leathery</appearance>
				<iconPath>PortableBlueprint/TentDoor/TentDoor_MenuIcon/TentDoor_Leathery</iconPath>
			</li>
		</uiIconPathsStuff>
		<costStuffCount>15</costStuffCount>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<designationCategory>Furniture</designationCategory>
		<drawerType>MapMeshAndRealTime</drawerType>
		<building>
			<isPlaceOverableWall>true</isPlaceOverableWall>
			<isEdifice>true</isEdifice>
			<blueprintGraphicData>
				<texPath>PortableBlueprint/TentDoor/TentDoor_Blueprint</texPath>
				<graphicClass>Graphic_Single</graphicClass>
			</blueprintGraphicData>
		</building>
		<modExtensions>
			<li Class="PortableBlueprint.Tent.TentDoorGraphics">
				<graphicDataTop>
					<texPath>PortableBlueprint/TentDoor/TentDoor_Top_Atlas</texPath>
					<graphicClass>PortableBlueprint.Tent.Graphic_Tent</graphicClass>
					<linkType>7</linkType>
					<damageData>
						<enabled>false</enabled>
					</damageData>
				</graphicDataTop>
				<graphicDataMover>
					<li>
						<texPath>PortableBlueprint/TentDoor/TentDoor_Mover1_Atlas</texPath>
						<graphicClass>PortableBlueprint.Tent.Graphic_Tent</graphicClass>
						<linkType>7</linkType>
						<damageData>
							<enabled>false</enabled>
						</damageData>
					</li>
					<li>
						<texPath>PortableBlueprint/TentDoor/TentDoor_Mover2_Atlas</texPath>
						<graphicClass>PortableBlueprint.Tent.Graphic_Tent</graphicClass>
						<linkType>7</linkType>
						<damageData>
							<enabled>false</enabled>
						</damageData>
					</li>
				</graphicDataMover>
			</li>
			<li Class="PortableBlueprint.Tent.TentParts"></li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="BuildingBase">
		<defName>PB_TentFloor</defName>
		<label>tent floor</label>
		<description>A mat to be placed inside the tent. It can prevent darting by the ground.</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<minifiedDef>PB_MinifiedThing_20Stackable</minifiedDef>
		<thingCategories>
			<li>Buildings</li>
		</thingCategories>
		<graphicData>
			<texPath>PortableBlueprint/TentFloor</texPath>
			<graphicClass>Graphic_Terrain</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
				<edgeTop>Damage/Edge</edgeTop>
				<edgeBot>Damage/Edge</edgeBot>
				<edgeLeft>Damage/Edge</edgeLeft>
				<edgeRight>Damage/Edge</edgeRight>
			</damageData>
		</graphicData>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<WorkToBuild>20</WorkToBuild>
			<Mass>0.4</Mass>
			<Flammability>1.0</Flammability>
		</statBases>
		<costStuffCount>5</costStuffCount>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>FloorEmplacement</altitudeLayer>
		<passability>Standable</passability>
		<blockWind>false</blockWind>
		<castEdgeShadows>false</castEdgeShadows>
		<fillPercent>0</fillPercent>
		<coversFloor>false</coversFloor>
		<placingDraggableDimensions>2</placingDraggableDimensions>
		<tickerType>Never</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>true</neverMultiSelect>
		<useStuffTerrainAffordance>true</useStuffTerrainAffordance>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<holdsRoof>false</holdsRoof>
		<designationCategory>Furniture</designationCategory>
		<uiOrder>8003</uiOrder>
		<blockLight>false</blockLight>
		<canOverlapZones>true</canOverlapZones>
		<fertility>0</fertility>
		<wipesPlants>true</wipesPlants>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<building>
			<paintable>true</paintable>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
			<ai_chillDestination>false</ai_chillDestination>
			<supportsWallAttachments>false</supportsWallAttachments>
			<blueprintGraphicData>
				<texPath>Things/Special/TerrainBlueprint</texPath>
				<graphicClass>Graphic_Single</graphicClass>
			</blueprintGraphicData>
			<uninstallWork>2</uninstallWork>
		</building>
		<damageMultipliers>
			<li>
				<damageDef>Bomb</damageDef>
				<multiplier>3</multiplier>
			</li>
			<li>
				<damageDef>Thump</damageDef>
				<multiplier>3</multiplier>
			</li>
		</damageMultipliers>
		<placeWorkers>
			<li>PortableBlueprint.Tent.PlaceWorker_TentFloor</li>
		</placeWorkers>
		<modExtensions>
			<li Class="PortableBlueprint.Tent.TentParts">
				<filthAcceptanceMask>Any</filthAcceptanceMask>
			</li>
		</modExtensions>
	</ThingDef>
	
</Defs>